{
  "name": "concept--number-input-for-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "run-s -c lint:*",
    "lint:eslint": "eslint --cache .",
    "lint:prettier": "prettier --cache --check .",
    "lint:tsc": "tsc --noEmit",
    "lint-fix": "run-s -c lint-fix:*",
    "lint-fix:eslint": "npm run lint:eslint --fix",
    "lint-fix:prettier": "prettier --cache --write ."
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.22.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.7"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  }
}
